# Production Environment Configuration
# Replace these values with your actual production values

# Backend API Configuration
VITE_API_BASE_URL=https://api.yourproductiondomain.com/api
VITE_SOCKET_URL=https://api.yourproductiondomain.com

# Clerk Authentication Configuration
VITE_CLERK_PUBLISHABLE_KEY=pk_live_your_production_publishable_key_here
VITE_CLERK_SIGN_IN_URL=/login
VITE_CLERK_SIGN_UP_URL=/signup
VITE_CLERK_AFTER_SIGN_IN_URL=/app/feed
VITE_CLERK_AFTER_SIGN_UP_URL=/app/feed

# Feature Flags
VITE_ENABLE_PWA=false
VITE_ENABLE_NOTIFICATIONS=true

# Environment
VITE_NODE_ENV=production

# Error Tracking (Sentry)
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=production
